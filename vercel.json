{
  "version": 2,
  "builds": [
    { "src": "js/server.js", "use": "@vercel/node" },
    { "src": "public/index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/js/(.*)", "dest": "js/server.js" },
    { "src": "/", "dest": "public/index.html" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" }
  ]
}
